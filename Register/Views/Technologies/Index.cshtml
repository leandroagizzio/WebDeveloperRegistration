@model IEnumerable<Register.Models.Technology>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TechnologyName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.TechnologyName, "Index", "Developers", new { technology = item.TechnologyName }, null)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TechnologyId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TechnologyId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TechnologyId })
        </td>
    </tr>
}

</table>
